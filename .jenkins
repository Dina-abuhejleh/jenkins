pipeline {
    agent any
    stages {
        stage('Build') {
            steps { 
                 sh 'hostname'
                 sh 'free -m'
                 sh "free -m | awk 'NR==2{printf \"%.2f",\$3*100/\$2}'"
                 echo "Running ${env.BUILD_ID}"
            }
        }     
    }
}
